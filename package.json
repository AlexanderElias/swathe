    "url": "git+https://github.com/AlexanderElias/swathe.git"
  },
  "keywords": [
    "data",
    "bind",
    "binding",
    "data-binding",
    "observe",
    "observables",
    "observeable",
    "object",
    "2-way",
    "2",
    "way"
  ],
