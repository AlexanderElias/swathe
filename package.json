{
  "name": "swathe",
  "version": "2.0.0",
  "description": "Swathe - Data Binding | No Dirty Checks | No Virtual DOM | Powerful | Small",
  "main": "dist/swathe.js",
  "module": "src/swathe.b.m.js",
  "scripts": {
	"swathe": "muleify pack src dist -m",
