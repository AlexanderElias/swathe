  "bugs": {
    "url": "https://github.com/AlexanderElias/swathe/issues"
  },
  "homepage": "https://github.com/AlexanderElias/swathe#readme",
  "dependencies": {
    "erbium": "^1.4.6",
    "obsr": "^1.0.3"
  }
}
